<div class="col-md-6">
<div class="box box-info" style="margin-top:10px">
               <!-- <div class="box-header with-border">
                  <h3 class="box-title">Horizontal Form</h3>
                </div>-->
                <!-- /.box-header -->
                <!-- form start -->
               <div class="form-horizontal">
   

              <%= form_for @project, method: :post, url: project_list_index_path do |f| %>           
                  <div class="box-body">
         
                    <div class="form-group">
                      <label for="Name" class="col-sm-3" >Name</label>
                      <div class="col-sm-5">
                      
                        <%= f.text_field  :name %>
                      </div>
                    </div>
                    <div class="form-group">
                      <label for="datetimepicker1" class="col-sm-3 ">Start at</label>
                      <div class="col-sm-5">
                      
                          <%= f.datetime_field :end_at, :class => 'datetimepicker1', :size => 10, :maxlength => 10 %>
                      </div>

                    </div>

                    <div class="form-group">
                      <label for="datetimepicker2" class="col-sm-3 ">end at</label>
                      <div class="col-sm-5">
                     
                        <%= f.datetime_field :end_at %>
                       
                          
                      </div>
                          
                    
                     </div>
                    </div>
                    <div class="form-group">
                      <label for="Name" class="col-sm-3 ">Notice</label>
                      <div class="col-sm-5">
                           <%= f.text_field :description%>

                      </div>
                    </div>
                    <div class="form-group">
                      <label for="Name" class="col-sm-3">Status</label>
                      <div class="col-sm-5">
                      
                       <%= f.text_field  :status %>
                       
                      </div>
                    </div>

                   
                    
                 
                  </div><!-- /.box-body -->

                  <div class="box-footer">
                    <!--<button  type="submit" class="btn btn-primary">Save</button>-->
                    <%= f.submit "create", :class => "btn btn-primary" %>
                  </div>
                   <% end %>
                  <!-- /.box-footer -->
                 </div>
                
                 
               
              </div>
</div>

<script type="text/javascript">
            $(function () {
                $('#datetimepicker1').datepicker();
              });
</script>
